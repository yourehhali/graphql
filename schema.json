{
  "tables": {
    "audit": {
      "description": "Some exercises require reviews from other students or any other member of the school. The reviewer checks that some conditions are met by the code of the student being reviewed.",
      "columns": [
        "id",
        "groupId",
        "auditorId",
        "attrs",
        "grade",
        "createdAt",
        "updatedAt",
        "code",
        "resultId",
        "version",
        "endAt",
        "private",
        "discordToken"
      ]
    },
    "event": {
      "description": "An event anchors an objects into time.",
      "columns": [
        "id",
        "createdAt",
        "endAt",
        "registrationId",
        "objectId",
        "parentId",
        "status",
        "path",
        "campus",
        "code"
      ]
    },
    "event_user": {
      "description": "Links users and events.",
      "columns": [
        "id",
        "createdAt",
        "userId",
        "eventId"
      ]
    },
    "group": {
      "description": "Projects are required to be made by groups.",
      "columns": [
        "id",
        "objectId",
        "eventId",
        "captainId",
        "createdAt",
        "updatedAt",
        "status",
        "path",
        "campus"
      ]
    },
    "group_status": {
      "columns": ["status"]
